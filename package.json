{
  "name": "whatsapp-tltr-bot",
  "version": "1.0.0",
  "description": "Um aplicativo Express que usa a biblioteca whatsapp-web.js",
  "main": "src/index.mjs",
  "scripts": {
    "start": "node -r tsconfig-paths/register dist/server.js",
    "dev": "ts-node-dev --require tsconfig-paths/register --inspect --transpile-only --ignore-watch node_modules --respawn  src/server.ts",
    "dev-debug": "ts-node-dev --inspect --require tsconfig-paths/register --transpile-only --ignore-watch node_modules --respawn  src/server.ts",
    "docker-dev": "docker compose -f docker-compose-dev.yml up",
    "docker-compose-dev": "docker-compose -f docker-compose-dev.yml up",
    "test": "jest",
    "test:usecase": "jest --testPathPattern=src/useCases",
    "build": "node -r tsconfig-paths/register ./node_modules/.bin/tsc",
    "morph": "ts-node-dev --require tsconfig-paths/register src/utils/morph.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@google-cloud/language": "^6.2.0",
    "@google-cloud/vertexai": "^0.5.0",
    "atob": "^2.1.2",
    "axios": "^1.6.8",
    "crypto": "^1.0.1",
    "dotenv": "^16.4.5",
    "express": "^4.18.2",
    "futoin-hkdf": "^1.5.3",
    "google-auth-library": "^9.6.3",
    "jest": "^29.7.0",
    "pg": "^8.11.5",
    "qrcode-terminal": "^0.12.0",
    "reflect-metadata": "^0.2.2",
    "ts-jest": "^29.1.2",
    "ts-morph": "^22.0.0",
    "tsconfig-paths": "^4.2.0",
    "tsyringe": "^4.8.0",
    "whatsapp-web.js": "^1.23.1-alpha.5"
  },
  "devDependencies": {
    "@tsconfig/node20": "^20.1.4",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.12.4",
    "@types/pg": "^8.11.5",
    "@types/qrcode-terminal": "^0.12.2",
    "@typescript-eslint/eslint-plugin": "^7.5.0",
    "@typescript-eslint/parser": "^7.5.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "mock-fs": "^5.2.0",
    "prettier": "^3.2.5",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.4.3"
  }
}
