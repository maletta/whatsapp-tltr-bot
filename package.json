{
  "name": "whatsapp-tltr-bot",
  "version": "1.0.0",
  "description": "Um aplicativo Express que usa a biblioteca whatsapp-web.js",
  "main": "src/index.mjs",
  "scripts": {
    "start": "node dist/src/server.js",
    "devjs": "nodemon src/index.mjs",
    "dev": "ts-node-dev --require tsconfig-paths/register --inspect --transpile-only --ignore-watch node_modules --respawn  src/server.ts",
    "docker-dev": "docker compose -f docker-compose-dev.yml up",
    "docker-compose-dev": "docker-compose -f docker-compose-dev.yml up",
    "test": "jest",
    "test2": "ts-node-dev --require tsconfig-paths/register --inspect --transpile-only --ignore-watch node_modules --respawn  src/test.ts",
    "build": "node -r tsconfig-paths/register ./node_modules/.bin/tsc"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@google-cloud/language": "^6.2.0",
    "@google-cloud/vertexai": "^0.5.0",
    "axios": "^1.6.7",
    "dotenv": "^16.4.5",
    "express": "^4.18.2",
    "google-auth-library": "^9.6.3",
    "jest": "^29.7.0",
    "qrcode-terminal": "^0.12.0",
    "ts-morph": "^22.0.0",
    "whatsapp-web.js": "^1.23.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/node": "^20.10.5",
    "@types/qrcode-terminal": "^0.12.2",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "eslint": "^8.56.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-import-helpers": "^1.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "nodemon": "^3.0.3",
    "prettier": "^3.2.5",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.3.3"
  }
}
